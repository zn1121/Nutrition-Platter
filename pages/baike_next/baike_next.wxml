<view>

<!-- 菜名 -->
<text class='caiming'>{{arr_search_show.name}}</text>
<!-- 归属地、菜系 -->
<view class='attribute'>
<text class='guishudi'>归属地：{{arr_search_show.area}}</text>
<text class='caixi'>菜系：{{arr_search_show.caixi}}</text>
</view>

<!-- 图片 -->

<view class='img_box'>
<image class='img' src='{{arr_search_show.url}}' mode='scaleToFill'></image>
</view>

<!-- 标语 -->
<view class='tip_box'>
<text class='tip'>{{arr_search_show.tip}}</text>
</view>

<!-- 文化介绍 -->
<view class='culture'>
<text class='content'>{{arr_search_show.culture}}</text>
</view>


</view>